{
  "name": "@cycler/express",
  "version": "1.0.0-beta.0",
  "description": "Express driver for Cycle.js.",
  "repository": "http://github.com/whitecolor/cycler",
  "keywords": [
    "cyclejs",
    "driver",
    "express",
    "http"
  ],
