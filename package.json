{
  "name": "cycler",
  "license": "ISC",
  "private": true,
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "workspaces": [
    "*"
  ],
  "scripts": {
    "precommit": "yalc retreat --all && yalc check",
    "postcommit": "yalc update",
    "propagate-config": "node .scripts/propagate-config",
    "test": "node .scripts/test",
    "watch": "node .scripts/test --watch",
    "release": "node .scripts/docker-run \"node .scripts/release-\"",
    "docker-test": "node .scripts/docker-run \"node .scripts/test\"",
    "docker-watch": "node .scripts/docker-run \"node .scripts/test --watch\"",
    "clean-built": "del-cli **/*.js **/*.js.map **/*.d.ts \"!node_modules/**\"",
    "install-deps": "node .scripts/install-deps",
    "build-watch": ".scripts/cleanup-built-files.sh && .scripts/wait-install-deps.sh && .scripts/fix-tsc-watch.sh && tsc -w",
    "ci": "yarn && tsc -p .scripts && yarn install-deps && tsc && yarn docker-test"
  },
  "dependencies": {
    "@cycle/isolate": "^2.1.0",
    "@cycle/most-run": "^7.1.0",
    "@cycle/run": "^4.1.0",
    "@cycle/rxjs-run": "^7.0.0",
    "@cycler/task": "link:task",
    "@types/express": "^4.0.32",
    "@types/fs-extra": "^2.1.0",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^2.2.41",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/superagent": "^2.0.32",
    "@types/supertest": "^1.1.30",
    "@types/tape": "^4.2.27",
    "browserify": "^13.0.1",
    "budo": "^8.4.0",
    "del-cli": "^0.2.1",
    "express": "^4.15.2",
    "fs-extra": "^2.1.2",
    "get-port": "^2.1.0",
    "husky": "^0.13.3",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "most": "^1.0.1",
    "node-dev": "^3.1.3",
    "ramda": "^0.23.0",
    "rxjs": "^5.2.0",
    "source-map-support": "^0.4.2",
    "supertest": "^2.0.0",
    "tap-browser-color": "^0.1.2",
    "tape": "^4.4.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "tslint": "^4.5.1",
    "typescript": "^3.5.3",
    "validate-commit-msg": "^2.8.2",
    "watchify": "^3.7.0",
    "xstream": "^11.2.0"
  },
  "devDependencies": {}
}
