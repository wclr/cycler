    "@cycle/rxjs-run": "^3.1.0",
    "@cycle/xstream-run": "^4.1.0",
    "@types/express": "^4.0.32",
    "@types/ramda": "0.0.1",
    "@types/superagent": "^2.0.32",
    "@types/supertest": "^1.1.30",
    "@types/tape": "^4.2.27",
    "browserify": "^13.0.1",
    "budo": "^8.4.0",
    "eachdir": "^1.0.1",
    "express": "^4.14.0",
    "get-port": "^2.1.0",
    "ghooks": "^1.3.2",
    "most": "^1.0.1",
    "node-dev": "^3.1.3",
    "ramda": "^0.22.1",
    "rx": "^4.0.7",
    "rxjs": "^5.0.0-rc.4",
    "source-map-support": "^0.4.2",
    "supertest": "^2.0.0",
    "tap-browser-color": "^0.1.2",
    "tape": "^4.4.0",
    "typescript": "next",
