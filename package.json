    "xstream": "^9.0.0"
  },
  "scripts": {
    "up": "docker-compose up",    
    "up:express": "docker-compose up express-test",
    "up:forage": "docker-compose up forage-test",
    "up:hmr": "docker-compose up hmr-test",
    "up:http": "docker-compose up hmr-http",
    "up:mongoose": "docker-compose up mongoose-test",
    "up:spy": "docker-compose up spy-test",
    "up:task": "docker-compose up task-test",
    "install-typings": "typings install",
    "test-ci": "yarn && typings install && sh ./.scripts/ci-all-packages.sh",
    "build-dev-image": "docker build -t cycler-dev-node .",
    "rebuild-dev-image": "docker build -t cycler-dev-node --no-cache ."
  },
  "_config": {
    "ghooks": {
      "commit-msg": "node ./node_modules/.bin/validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": "@northbrook/commit-types"
    }
  }
}
